  "scripts": {
    "build": "tsc -p .",
    "db:create": "yarn build && node ./tools/create-db.js",
    "db:delete": "docker-compose -f docker-compose.local.yml down -v",
    "db:sync": "yarn build && node ./tools/sync-db.js",
    "db:seed": "yarn build && node ./tools/seed-db.js up",
    "db:seed:undo": "yarn build && node ./tools/seed-db.js down",
    "db:migrate": "yarn build && node ./tools/migrate-db.js up",
    "db:migrate:undo": "yarn build && node ./tools/migrate-db.js down",
    "start:env": "docker-compose -f docker-compose.voter.storages.yml up -d --force-recreate",
    "start:core": "cp .envLOCAL .env && yarn build && ts-node --files ./src/services/core/bin/www.ts --out-put dist",
    "run:wallet": "ts-node ./src/services/wallet/wallet.ts",
    "run:send": "ts-node ./src/services/wallet/send.js",
    "raw:core": "node ./dist/src/services/core/bin/www.js",
    "image": "cp .envDEV .env && yarn build && docker image build --tag somebeyti/voter:latest .",
    "container": "docker-compose -f docker-compose.voter.app.yml up",
    "login": "docker login --username=somebeyti --password=nahuipass1488",
    "taging": "docker tag 69ecad482cab somebeyti/voter:latest",
    "push": "docker push somebeyti/voter:latest"
  },
